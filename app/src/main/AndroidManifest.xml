<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.graduateproj">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GraduateProj">
        <activity
            android:name=".mainPack.ui.MainContentActivity"
            android:exported="false"
            android:label="@string/title_activity_main_content" />
        <activity
            android:name=".splashPack.SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.GraduateProj.CustomizeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".loginPack.ui.MainLoginActivity"
            android:exported="false" />
    </application>

</manifest>